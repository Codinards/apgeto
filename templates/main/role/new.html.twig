{% extends 'base.html.twig' %}

{% block title %}New UserAction{% endblock %}

{% block body %}
    <h1>Create new UserAction</h1>
    <div class="row">
        <div class="col-md-12 text-right">
            {{ create_link('app_backend_user_action_index', __t('user.all.action.index'), {_locale: locales.locale}, {class: 'btn btn-info mb-3'}) }}
        </div>
    </div>
    <div class="row">
        <div class="col mx-auto">
            <div class="card">
                <div class="card-header bg-edit">
                    <h3 text-center>{{ __U('role.creation')}}</h3>
                   
                </div>
                <div class="card-body bg-save">
                    {{ include(viewPath ~ '/_form.html.twig') }}
                </div>
            </div>
            
        </div>
    </div>
{% endblock %}
